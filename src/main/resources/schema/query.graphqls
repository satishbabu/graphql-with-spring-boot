type Query {
	firstQuery : String
	secondQuery : String
	fullName(sampleRequest : SampleRequest) : String
	Student(id : Long) : StudentResponse
}

input SampleRequest {
	firstName : String!
	lastName : String
}

type StudentResponse {
	id : Long
	firstName : String
	lastName : String
	email : String
	street : String
	city : String
	fullName : String
	learningSubjects : [SubjectResponse]
}

type SubjectResponse {
	id : Long
	subjectName : String
	marksObtained : Float
}